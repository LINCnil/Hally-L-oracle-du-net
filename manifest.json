{
	"name": "Hally l'Oracle du Net",
	"short_name": "Hally",
	"version": "0.7",
	"manifest_version": 2,
	"description": "révélateur d'algorithmes",
	"homepage_url": "https://oracledu.net",
	"permissions": [
		"activeTab",
		"tabs",
		"storage"
	],
	"background":{
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["https://twitter.com/*"],
			"js":["messengertwitter.js"],
			"run_at":"document_start"
		},
		{
			"matches": ["https://www.facebook.com/*", "https://www.google.fr/*", "https://www.google.com/*"],
			"js":["messengerfacebook.js"],
			"run_at":"document_start"
		},
		{
			"matches": ["https://www.google.fr/*", "https://www.google.com/*"],
			"js":["messengergoogle.js"],
			"run_at":"document_start"
		}
	],
	"browser_action": {
		"default_title": "L'oracle vous révèle comment les algorithmes vous calculent",
		"default_icon": "icons/oraclesleep.png",
		"default_popup": "src/popup2.html",
		"icons": {
			"16": "icons/oracle16.png",
			"32": "icons/oracle32.png",
			"32": "icons/oracle48.png",
			"38": "icons/oracle38.png",
			"128": "icons/oracle128.png",
			"512": "icons/oracle512.png"
		}
	},
	"web_accessible_resources": [
		"icons/oracle32.png",
		"icons/oraclesleep.png",
		"src/css/style.css",
		"src/css/cursor.cur",
		"src/css/cursor2.cur",
		"src/css/lsdpointer.cur",
		"src/img/aspiration.png",
		"src/img/aspiration.gif",
		"src/img/attention.png",
		"src/img/attention.gif",
		"src/img/attentionbig.gif",
		"src/img/bulle.jpg",
		"src/img/bulle.gif",
		"src/img/datamoney.png",
		"src/img/datamoney.gif",
		"src/img/moneybig.gif",
		"src/img/gouvernance.jpg",
		"src/img/governancebig.jpg",
		"src/img/gouvernance.gif",
		"src/img/highway.png",
		"src/img/highway.gif",
		"src/img/sagesse.png",
		"src/img/sagesse.gif",
		"src/img/sagessebig.gif",
		"src/img/selfie.png",
